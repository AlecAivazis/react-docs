{
  "name": "react-docs",
  "version": "0.0.2",
  "description": "A utility to generate a list documentation for every component exported by a package.",
  "main": "build/index.js",
  "scripts": {
    "build": "rm -rf build  && babel src --out-dir build --ignore **/test.js",
    "test": "NODE_ENV=test jest"
  },
  "keywords": [
    "react",
    "docs",
    "flow",
    "flowtype"
  ],
  "author": "Alec Aivazis <alec@aivazis.com>",
  "license": "MIT",
  "jest": {
    "testRegex": "test.js$"
  },
  "prettier": {
    "tabWidth": 4,
    "semi": false,
    "singleQuote": true,
    "printWidth": 100
  },
  "dependencies": {
    "babylon": "^7.0.0-beta.44"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.44",
    "@babel/core": "^7.0.0-beta.44",
    "@babel/preset-env": "^7.0.0-beta.44",
    "@babel/preset-stage-1": "^7.0.0-beta.44",
    "jest-cli": "^22.4.3",
    "prettier": "^1.12.1"
  }
}
